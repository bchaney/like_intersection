<div class="container">
    <div class="span-24">
        <h1><%= @me["name"] %>'s friends</h1>
    </div>
    <div class="prepend-1"/>
<% @friends.each do |friend| %>
    <div class="span-6">
    <%= link_to(image_tag("http://graph.facebook.com/" + friend["id"] + "/picture", 
                          :class => "round"), 
                          likes_path(friend), friend) %><br>
            <%= link_to(friend["name"], likes_path(friend), friend) %>
    </div>
<% end %>
</div>
